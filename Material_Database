	// Gets the ISBN Check Hashmap to verify that the information used in ISBN search
	// is strictly an ISBN number instead of another means of book search
	public static Object getISBNCheck () {
		return f_BookISBNCheck;
		
	}
	
	// Gets the BookISBN Hashmap and information
	public static Object getISBN() {
		return f_BookISBN;
		
	}
	
	// Gets the Book Title Hashmap and information
	public static Object getTitle() {
		return f_BookTitle;
		
	}
	
	// Gets the Book Author Hashmap and information
	public static Object getAuthor() {
		return f_BookAuthor;
		
	}
	
	// Gets the Book Genre Hashmap and information
	public static Object getGenre() {
		return f_BookGenre;
		
	}
	
	// Gets the Book Quantity Hashmap and information
	public static Object getQuantity() {
		return f_BookQuantity;
		
	}
	
	// Gets the Material Type Hashmap and Information
	public static Object getMaterialType() {
		return f_MaterialType;
		
	}
	
}
